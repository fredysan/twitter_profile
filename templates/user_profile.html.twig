{#
/**
 * @file user_profile.html.twig
 * Default theme implementation to present User profile data.
 *
 * This template is used when viewing User profile pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_user_profile()
 *
 * @ingroup themeable
 */
#}
<div{{attributes.addClass('user_profile')}}>
  <div {{attributes.addClass('grid-container')}} style='display: flex'>
    <div {{attributes.addClass('profile-twitter')}}>
      {{ content['twitter_stream']}}
    </div>
    <div {{attributes.addClass('profile-info')}} style='padding-left: 20px'>
      {{ content.name }}
      {{ content.field_image }}
      {{ content.field_job_title}}
      {{ content.field_description}}
    </div>
  </div>
</div>
